    "import torch\n",
    "\n",
    "# Tính đạo hàm của vận tốc f(x) = x^2+2*x\n",
    "\n",
    "def _derivative(x):\n",
    "  f1 = x**2 + 2*x\n",
    "  delta = 0.001 # giá trị delta ở mẫu\n",
    "  x0 = x-delta\n",
    "  f0 = x0**2 + 2*x0\n",
    "  der = (f1-f0)/delta\n",
    "  return der\n",
    "\n",
    "_derivative(10)"
   ]
  }
